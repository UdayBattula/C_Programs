#include<stdio.h>
main()
{
    int n,s=0;
    printf("Enter the size of array: ");
    scanf("%d",&n);
    int mean[n];
    for(int i=0;i<n;i++)
    {
        printf("Enter the %d number: ",i+1);
        scanf("%d",&mean[i]);
        s=s+mean[i];
    }
    float m;
    m=s/(n-1);
    printf("%d is the mean");
}
