#include<stdio.h>
int test (int a);
main()
{	int n,c;
	printf("enter any number= ");
	scanf("%d",&n);
	c=test(n);
	if(c==1)
		printf("%d is not a prime",n);
	else if(c==2)
	printf("%d is niether prime nor a composite ",n);
	else
		printf("%d is prime",n);
}
	int test (int a)
	{	int i=2,c=0;
	if(a==1)
	return 2;
	else
	{	
		for( ;i<a;i++)
		{
			if(a%i==0)
			return 1;
		}
	}
		return 0;
	}
