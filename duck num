# include <stdio.h>
int main()
	{
int dno,d,r,c=0;
printf("Enter any number: ");
scanf("%d",&d);
dno=d;
 while(d>0)
		{
            if(d%10==0)
            c=1;
			d/=10;
        }
            if(dno>0&&c==1)
            printf("is a Duck Number\n");
            else
            printf("is not a Duck Number\n");
	}
